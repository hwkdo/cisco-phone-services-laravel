{
    "name": "hwkdo/cisco-phone-services-laravel",
    "description": "This is my package cisco-phone-services-laravel",
    "keywords": [
        "hwkdo",
        "laravel",
        "cisco-phone-services-laravel"
    ],
    "homepage": "https://github.com/hwkdo/cisco-phone-services-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "hwkdo",
            "email": "doit@hwk-do.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0||^12.0"
    },
    "require-dev": {
        
    },
    "autoload": {
        "psr-4": {
            "Hwkdo\\CiscoPhoneServicesLaravel\\": "src/",
            "Hwkdo\\CiscoPhoneServicesLaravel\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hwkdo\\CiscoPhoneServicesLaravel\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hwkdo\\CiscoPhoneServicesLaravel\\CiscoPhoneServicesLaravelServiceProvider"
            ],
            "aliases": {
                "CiscoPhoneServicesLaravel": "Hwkdo\\CiscoPhoneServicesLaravel\\Facades\\CiscoPhoneServicesLaravel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}